{
  "name": "zeit-toast-clone",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Ricardo Q. Bazan",
  "main": "dist/index.js",
  "module": "dist/zeit-toast-clone.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "styles.css"
  ],
  "scripts": {
    "build": "tsdx build",
    "check:lint": "tsdx lint",
    "check:types": "tsc --noEmit",
    "fix:format": "yarn prettier --write",
    "fix:lint": "tsdx lint --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,ts,tsx}\"",
    "size": "size-limit",
    "storybook": "start-storybook",
    "test:static": "run-p check:*",
    "test:unit": "tsdx test --passWithNoTests",
    "prepare": "husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "tsdx lint --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addons": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/styled-components": "^5.1.24",
    "babel-loader": "^8.2.4",
    "eslint-plugin-prettier": "3.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-spring": "^8.0.0",
    "size-limit": "^7.0.8",
    "styled-components": "^5.3.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17",
    "react-spring": "^8.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "size-limit": [
    {
      "path": "dist/vercel-toast.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/vercel-toast.esm.js",
      "limit": "10 KB"
    }
  ]
}
